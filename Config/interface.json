{
    "shutdown":{
        "name":"shutdown",
        "description":"Exits current runtime of bot client",
        "ephemeral":true,
        "installs":{
            "guilds":true,
            "users":true
        },
        "contexts":{
            "guilds":true,
            "dms":true,
            "private_channels":true
        },
        "embed":{
            "pass":{
                "title":":white_check_mark: Shutdown Successful",
                "description":"Feeling sleepy...",
                "colour":"green"
            },
            "fail":{
                "title":":warning: Shutdown Failed",
                "description":"You do not have permission to shut down the bot.",
                "colour":"red"
            }
        }
    },
    "start":{
        "name":"start",
        "description":"Notification on new bot client runtime",
        "ephemeral":true,
        "installs":{
            "guilds":true,
            "users":true
        },
        "contexts":{
            "guilds":true,
            "dms":true,
            "private_channels":true
        },
        "embed":{
            "pass":{
                "title":":white_check_mark: Start Successful",
                "description":"Good morning!",
                "colour":"green"
            },
            "fail":{
                "title":":warning: Start Failed",
                "description":"Something went wrong.",
                "colour":"red"
            }
        }
    }
}