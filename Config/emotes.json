{
    "steal":{
        "name":"steal",
        "description":"Adds an emote from the selected message to a mutual server",
        "ephemeral":true,
        "installs":{
            "guilds":true,
            "users":true
        },
        "contexts":{
            "guilds":true,
            "dms":true,
            "private_channels":true
        },
        "embed":{
                "title":":open_file_folder: Steal",
                "description":"Add an emote to a server",
                "colour":"gold"
        },
        "select":{
            "emotes":{
                "placeholder":"Select an emote...",
                "min_values":1,
                "max_values":1
            },
            "guilds":{
                "placeholder":"Select a server...",
                "min_values":1,
                "max_values":1
            }
        },
        "scripts":{
            "emote_pattern":"<a?:[a-zA-Z0-9_~]+:[0-9]+>",
            "name_pattern":"(?:[a-zA-Z0-9_~]+)[a-zA-Z0-9_~]+(?=:)"
        }
    }
}